class Solution {
    public int reverse(int x) {
      long  rev=0;
        while(x!=0) {
            rev=rev*10;
             rev+=x%10; 
            if(rev > Integer.MAX_VALUE) return 0;
            if(rev < Integer.MIN_VALUE) return 0;
            x=x/10;
        }
        return (int) rev;
    }
};
